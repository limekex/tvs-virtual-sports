{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "tvs-virtual-sports/my-activities",
  "title": "TVS My Activities",
  "category": "widgets",
  "icon": "list-view",
  "description": "Display user activities list",
  "keywords": ["tvs", "activities", "virtual sports"],
  "textdomain": "tvs-virtual-sports",
  "supports": {
    "html": false,
    "multiple": true,
    "inserter": true
  },
  "attributes": {
    "routeId": {
      "type": "number",
      "default": 0
    },
    "limit": {
      "type": "number",
      "default": 5
    },
    "title": {
      "type": "string",
      "default": "Recent Activities"
    }
  },
  "render": "file:./render.php"
}
